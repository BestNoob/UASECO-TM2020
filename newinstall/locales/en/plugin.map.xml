<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<locales>
	<!--
	Version:	2019-10-03
	Authors:	undef.de
	//-->
<!-- plugin messages -->
	<plugin_description>Times playing time of a map, provides map and time info and shows (file)names of current map\'s and song mod.</plugin_description>
	<slash_chat_map_description>Shows info about the current map</slash_chat_map_description>
	<slash_chat_song_description>Shows filename of current map\'s song</slash_chat_song_description>
	<slash_chat_mod_description>Shows (file)name of current map\'s mod</slash_chat_mod_description>
	<slash_chat_nextmap_description>Shows name of the next map</slash_chat_nextmap_description>
	<slash_chat_playtime_description>Shows time current map has been playing</slash_chat_playtime_description>
	<map>{#server}» Current map {#highlite}{1} {#server}by {#highlite}{2} {#server}Author: {#highlite}{3} {#server}Gold: {#highlite}{4} {#server}Silver: {#highlite}{5} {#server}Bronze: {#highlite}{6} {#server}Cost: {#highlite}{7}</map>
	<current_map>{#server}» Current map {#highlite}{1} {#server}by {#highlite}{2} {#server}Author: {#highlite}{3}</current_map>
	<next_map>{#server}» The next Map will be: {#timelite}[{1}] {#highlite}{2}</next_map>
	<song>{#server}» Map {#highlite}{1} {#server}plays song: {#highlite}{2}</song>
	<no_song>{#server}» {#error}No map song found! {1}</no_song>
	<no_song_proposal>Try {#highlite}$i /music current {#error}instead.</no_song_proposal>
	<mod>{#server}» Map {#highlite}{1} {#server}uses mod: {#highlite}{2} {#server}({#highlite}{3}{#server}){4}</mod>
	<no_mod>{#server}» {#error}No map mod found!</no_mod>
	<notonrelay>{#server}» {#error}Command unavailable on relay server</notonrelay>
	<playtime>{#server}» Current map {#highlite}{1}{#server} has been played for {#highlite}{2}{3}</playtime>
	<playtime_replay>{#server}({#highlite}{1}{#server} time(s) replayed, total {#highlite}{2}{#server})</playtime_replay>
	<playtime_finish>{#server}» Current map {#highlite}{1}{#server} finished after {#highlite}{2}</playtime_finish>
</locales>