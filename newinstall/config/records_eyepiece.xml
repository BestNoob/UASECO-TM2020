<settings>
	<!-- THIS IS THE CONFIG FILE FOR UASECO ONLY -->

	<!-- Here you can setup some special features -->
	<features>
		<!-- Amount of seconds the record widgets wait until they refresh if required (increase if you experience problems on populated servers) -->
		<refresh_interval>10</refresh_interval>

		<!-- If you want to mark at the Record-Widgets the current online Players records set to 'true', otherwise set 'false' -->
		<mark_online_player_records>true</mark_online_player_records>

		<!-- This allows you to illuminate all too dark colored names from $000 till $555 to $AAA; set to 'true' to enabled, otherwise set 'false' -->
		<illuminate_names>false</illuminate_names>

		<!-- Setup the formatting of numbers (e.g. Amount of Planets, Won...), pick on of the following:
			'english'	= 1,234.56
			'german'	= 1.234,56
			'french'	= 1 234,56
		-->
		<number_format>english</number_format>

		<!-- If you want to shorten high numbers like 21,425 to 21k then set to 'true', otherwise set 'false' -->
		<shorten_numbers>false</shorten_numbers>

		<karma>
			<!-- With this you adjust the min. amount of Votes for a Map to come into the TopMaps list, Maps below this limit are skipped from TopMaps list. -->
			<min_votes>10</min_votes>

			<!-- Here you can select the method how the Karma is calculated:
				'tmkarma'	= calculate like TM-Karma.com it does (default)
				'rasp'		= calculate like XAseco with RASP/Karma
			//-->
			<calculation_method>tmkarma</calculation_method>
		</karma>

		<songlist>
			<!-- If you want to sort the Songlist, then set to 'true', otherwise (for the unsorted Songlist) 'false' -->
			<sorting>true</sorting>

			<!-- If set to 'true', then display the MusiclistWindow after unjuke one Song or clear the entire jukebox. To disable set to 'false' -->
			<force_songlist>true</force_songlist>
		</songlist>

		<maplist>
			<!-- What to display from a Map Author:
				'nickname'	= Author Nickname (default, only if available)
				'login'		= Author Login
			//-->
			<author_display>nickname</author_display>

			<!-- Sorting the Maplist:
				'author'	= Order by Authorname (default)
				'map'		= Order by Mapname
				'false'		= unsorted Maplist
			//-->
			<sorting>author</sorting>

			<!-- If set to 'true', then display the MaplistWindow after unjuke one Map or clear the entire jukebox. To disable set to 'false' -->
			<force_maplist>true</force_maplist>

			<!-- This feature is for Maps that are not at mania-exchange.com, but you want to see the map image in the "Last/Current/Next-Window" -->
			<mapimages>
				<!-- If set to 'true', then the Images from the Mapfile are used in the Window, otherwise set to 'false'. -->
				<enabled>true</enabled>

				<!--
				Setup in UASECO.xml the <mapimages_path> to a path which are accessible over HTTP for anybody.
 				e.g. 'http://www.example.com/mapimages/'
				//-->
				<access_url></access_url>
			</mapimages>
		</maplist>

		<!--
			Setup the limit of displayed amounts (and memory consumption) of all 'MORE RANKING'* lists,
			set to '-1' for no limit, or to a wanted amount (e.g. 5000 entries)
			*) Not used for Dedimania-Records, Local-Records, Live-Rankings and Top-Maps!
		//-->
		<toplist_limit>-1</toplist_limit>
	</features>


	<!-- Display a progress indicator for functions that need a little bit of time to load -->
	<show_progress_indicator>
		<!-- If the amount of Maps is larger then this configured value, then display the progress indicator. Set to '0' to disable -->
		<maplist>20</maplist>
	</show_progress_indicator>


	<!-- Styles of Widgets and Windows -->
	<style>
		<!-- Styles used for the Widgets at Race -->
		<widget_race>
			<!-- Background-Color, Format: RRGGBBAA (A = Alphachanal, Opacity) -->
			<background_default>55556655</background_default>
			<background_focus>55556688</background_focus>

			<background_style>Bgs1InRace</background_style>
			<background_substyle>BgEmpty</background_substyle>

			<border_style>Bgs1InRace</border_style>
			<border_substyle>BgEmpty</border_substyle>	<!-- Only 'BgTitleGlow', 'BgTitleShadow', 'Shadow', or 'BgList' are useful) -->

			<!-- If <title_background> is not empty then the style/substyle are ignored -->
			<title_background>0099FFDD</title_background>
			<title_style>BgsPlayerCard</title_style>
			<title_substyle>BgRacePlayerName</title_substyle>

			<!-- Style for the top X players entries in the records Widgets, if <top_background> is not empty then the style/substyle are ignored -->
			<top_background>55557799</top_background>
			<top_style>BgsPlayerCard</top_style>
			<top_substyle>BgCardSystem</top_substyle>

			<!-- Style for the highlite entries in the records Widgets, if <highlite_self_background> is not empty then the style/substyle are ignored -->
			<highlite_self_background>66880077</highlite_self_background>
			<highlite_self_style>BgsPlayerCard</highlite_self_style>
			<highlite_self_substyle>BgCardSystem</highlite_self_substyle>

			<!-- Style for the highlite entries in the records Widgets, if <highlite_other_background> is not empty then the style/substyle are ignored -->
			<highlite_other_background>88000055</highlite_other_background>
			<highlite_other_style>BgsPlayerCard</highlite_other_style>
			<highlite_other_substyle>BgCardSystem</highlite_other_substyle>

			<!-- Style for the entries at Rounds (+25) or Laps (time) in the records Widgets, if <highlite_common_background> is not empty then the style/substyle are ignored -->
			<highlite_common_background>22223388</highlite_common_background>
			<highlite_common_style>BgsPlayerCard</highlite_common_style>
			<highlite_common_substyle>ProgressBar</highlite_common_substyle>

			<!-- Here you can set all possible Formating-Codes of TM, but do not enter colors here! -->
			<formatting_codes></formatting_codes>

			<!-- Textcolor (NO formating Codes!); Format: RRGGBBAA (A = Alphachanal, Opacity) -->
			<colors>
				<default>FFFFFFFF</default>
				<scores>DDDDDDFF</scores>
				<top>FFFF00FF</top>
				<better>FF0000FF</better>
				<worse>CCCCCCFF</worse>
				<self>33FF55FF</self>

				<!-- Highlite the Ranks, Score and Player, if unwished set to '00000000' -->
				<background_rank>00000000</background_rank>
				<background_score>00000000</background_score>
				<background_name>00000000</background_name>
			</colors>
		</widget_race>


		<!-- Styles used for the Widgets at Scoretable -->
		<widget_score>
			<!-- If <background_color> is not empty then the style/substyle are ignored -->
			<background_color>222233BB</background_color>
			<background_style>BgsPlayerCard</background_style>
			<background_substyle>ProgressBar</background_substyle>

			<!-- If <title_background> is not empty then the style/substyle are ignored -->
			<title_background>0099FFDD</title_background>
			<title_style>BgsPlayerCard</title_style>
			<title_substyle>BgRacePlayerName</title_substyle>

			<!-- Here you can set all possible Formating-Codes of TM, but do not enter colors here! -->
			<formatting_codes></formatting_codes>

			<!-- Textcolor (NO formating Codes!); Format: RRGGBBAA (A = Alphachanal, Opacity) -->
			<colors>
				<default>FFFFFFFF</default>
				<scores>DDDDDDFF</scores>
			</colors>
		</widget_score>


		<!-- Here you can adjust the colors of all the Windows; Format: RRGGBBAA (A = Alphachanal, Opacity) -->
		<window>
			<headline_textcolor>0099FFFF</headline_textcolor>

			<background_style>Bgs1InRace</background_style>
			<background_substyle>BgTitle2</background_substyle>

			<!-- Background-Color for the Content inside the Window; Format: RRGGBBAA (A = Alphachanal, Opacity) -->
			<content_bgcolor>00001188</content_bgcolor>

			<title_style>Bgs1InRace</title_style>
			<title_substyle>BgTitle3_3</title_substyle>

			<!-- This obscure all other displays -->
			<lightbox>
				<enabled>false</enabled>
				<bgcolor>00000088</bgcolor>
			</lightbox>
		</window>
	</style>



	<!-- Adjust the UserInterface settings. Please note, that other plugins can override your configuration (if they require them)! -->
	<ui_properties>
		<!-- Set to 'true' for the map name and author displayed in the top right of the screen when viewing the scores table, otherwise set 'false'. If <map_widget> is enabled, then disabled by default! -->
		<map_info>false</map_info>

		<!-- Set to 'true' if you want to see the automatic Round-Score-Panel at the right side, otherwise set 'false'. If <round_score> is enabled, then disabled by default! -->
		<round_scores>false</round_scores>

		<!-- Set to 'true' if you want to see the warm-up progression displayed on the right of the screen during warm-up, otherwise set 'false' -->
		<warmup>false</warmup>

		<!-- Set to 'true' if you want to see the progression box displayed on the top of the screen at the end of the map -->
		<endmap_ladder_recap>false</endmap_ladder_recap>

		<!-- Set to 'true' if you want to see the Lap indicator on the right of the screen on multilaps map -->
		<multilap_info>false</multilap_info>

		<!-- Current position in the map ranking displayed at the bottom right of the screen -->
		<position>false</position>

		<!-- Speed and distance raced displayed in the bottom right of the screen -->
		<speed_and_distance>false</speed_and_distance>

		<!-- Previous and best times displayed at the bottom right of the screen -->
		<personal_best_and_rank>false</personal_best_and_rank>

		<!-- Time of the players at the current checkpoint displayed at the bottom of the screen -->
		<checkpoint_list>false</checkpoint_list>

		<!-- Race time left displayed at the bottom right of the screen -->
		<countdown>
			<position>105.0,-66.5,5.0</position>	<!-- x,y,z -->
		</countdown>

		<!-- Current race chrono displayed at the bottom center of the screen -->
		<chrono>
			<position>95.5,-84.0,5.0</position>	<!-- x,y,z -->
		</chrono>
	</ui_properties>




