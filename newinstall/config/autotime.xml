<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<settings>
	<!--
	Set the multiplicator for author time (e.g. 6 x author
	time = new timelimit); set to 0 to disable function
	// -->
	<multiplicator>4</multiplicator>

	<!-- Set minimum timelimit in minutes -->
	<mintime>3.5</mintime>

	<!-- Set maximum timelimit in minutes -->
	<maxtime>5</maxtime>

	<!-- Set default timelimit in minutes if any error occurs -->
	<defaulttime>5</defaulttime>

	<!-- 2 = in message window, 1 = in chat, 0 = none -->
	<display>1</display>

	<message>{#server}Â» Set {1} timelimit for Map {#highlite}{2}{#server} to {#highlite}{3}{#server}, calculation based upon the author time of {#highlite}{4}{#server}</message>
</settings>
